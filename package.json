{
  "name": "autoatendcolab",
  "version": "1.0.9",
  "main": "main.js",
  "isBuildNow": true,
  "scripts": {
    "start": "electron .",
    "nodemon": "nodemon --exec electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "electron-builder build --win --publish never"
  },
  "repository": "https://github.com/edermcastro/autoatendcolab",
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "build": {
    "appId": "com.electronjs.autoatend",
    "productName": "AutoAtendColab",
    "directories": {
      "output": "./dist/win-unpacked"
    },
    "files": [
      "**/*"
    ],
    "win": {
      "asar": true,
      "target": "nsis",
      "icon": "icon.ico"
    },
    "nsis": {
      "deleteAppDataOnUninstall": true
    }
  },
  "keywords": [],
  "author": "Eder Moraes",
  "license": "ISC",
  "description": "Sistema auxiliar para colaboradores de autoatendimento",
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "electron-single-instance": "^0.0.2",
    "electron-updater": "^6.6.2",
    "jquery": "^3.7.1"
  },
  "devDependencies": {
    "electron": "^35.2.1",
    "electron-builder": "^26.0.12",
    "electron-reload": "^2.0.0-alpha.1",
    "nodemon": "^3.1.10"
  }
}
